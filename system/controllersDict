FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dsmcStateControllers
(
);

dsmcFluxControllers
(
);

application     dsmcFoam+;

nTerminalOutputs    10;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         4;

deltaT          7.0e-6;

writeControl    runTime;

writeInterval   7.0e-3;

purgeWrite      2;

writeFormat     ascii;

writePrecision  10;

writeCompression uncompressed;

timeFormat      general;

timePrecision   10;

runTimeModifiable yes;

adjustTimeStep  no;

